{ coffee }

PLACE
INTERNAL
  p0,  { initial }
  p1,
  p2,
  p3,
  p4;
INPUT
  E,
  T,
  C;

OUTPUT
  B;

INITIALMARKING
  p0:   1;

FINALMARKING
  p4:   1;

TRANSITION t1    { ?e }
CONSUME
  p0:   1,
  E: 1;
PRODUCE
  p1:   1;

TRANSITION t2    { ?c }
CONSUME
  C:    1,
  p1: 1;
PRODUCE
  p2:   1;

TRANSITION t3    { ?t }
CONSUME
  p1:   1,
  T : 1;
PRODUCE
  p3:   1;

TRANSITION t4    { !bl }
CONSUME
  p2:   1;
PRODUCE
  B:    1,
  p4 : 1;

TRANSITION t5    { !br }
CONSUME
  p3:   1;
PRODUCE
  B:    1,
  p4 : 1;

{ END OF FILE }
