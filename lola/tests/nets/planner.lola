  CanvasDown : toCanvasBool(c,true()),
  CanvasUsed : c;
PRODUCE
  Sent2Disp     : toDisplayDocumentBool(d,Document_1,true()),
  CanvasDown    : toCanvasBool(c,true()),  
  CanvasNotUsed : c,
  DocShown      : toDocumentCanvasBool(Document_1,c,false());

